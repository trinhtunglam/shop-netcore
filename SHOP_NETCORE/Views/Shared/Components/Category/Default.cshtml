@model List<SHOP_NETCORE.Models.ProductCategoryViewModel>

@{ 
    var lstProducer = (List<SHOP_NETCORE.Models.ProducerViewModel>)ViewBag.lstProducer;
}
<h3 class="subtitle">Danh Mục</h3>
<div class="box-category">
    <ul id="cat_accordion">
        @foreach (var item in Model)
        {
            <li>
                <a href="javascript:void(0)">@item.Name</a> <span class="down"></span>
                <ul>
                    @foreach (var producer in lstProducer)
                    {
                        <li>
                            <a href="sanpham-producer-@producer.Id">@producer.Name</a> @*<span class="down"></span>*@
                        </li>
                    }
                    
                   
                </ul>
            </li>
        }
        
       
    </ul>
</div>